@page
@model CreateDepartmentModel
@using Application_webOracle.Models

<h1>Ajouter un Département</h1>

<form method="post">
    <div class="form-group">
        <label for="DeptNo">DeptNo</label>
        <input type="number" class="form-control" id="DeptNo" name="DeptNo" required />
    </div>

    <div class="form-group">
        <label for="Dname">Nom du Département</label>
        <input type="text" class="form-control" id="Dname" name="Dname" required />
    </div>

    <div class="form-group">
        <label for="LOC">Lieu</label>
        <input type="text" class="form-control" id="LOC" name="LOC" required />
    </div>

    <button type="submit" class="btn btn-primary">Ajouter</button>
</form>
